wget -q -O /hive/bin/nvErr https://raw.githubusercontent.com/IIIMaxIII/nvXid/main/nvErr && sudo chmod 755 /hive/bin/nvErr && (sudo grep -qxF "@reboot /hive/bin/nvErr start" /hive/etc/crontab.root || sudo sh -c 'echo "@reboot /hive/bin/nvErr start" >> /hive/etc/crontab.root') && (sudo grep -qxF "*/5 * * * * /hive/bin/nvErr start" /hive/etc/crontab.root || sudo sh -c 'echo "*/5 * * * * /hive/bin/nvErr start" >> /hive/etc/crontab.root')

reboot
